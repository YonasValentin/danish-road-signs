{
  "name": "@yonas-valentin/danish-road-signs",
  "version": "1.0.3",
  "description": "Complete collection of Danish road signs in dual format (React Native + Web)",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./native": {
      "types": "./dist/native/index.d.ts",
      "default": "./dist/native/index.js"
    },
    "./native/*": {
      "types": "./dist/native/*/index.d.ts",
      "default": "./dist/native/*/index.js"
    },
    "./web": {
      "types": "./dist/web/index.d.ts",
      "default": "./dist/web/index.js"
    },
    "./web/*": {
      "types": "./dist/web/*/index.d.ts",
      "default": "./dist/web/*/index.js"
    },
    "./svg/*": "./dist/svg/*",
    "./metadata": "./dist/metadata.json",
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "default": "./dist/constants.js"
    }
  },
  "scripts": {
    "organize": "tsx scripts/organize-svgs.ts",
    "generate:native": "tsx scripts/generate-native.ts",
    "generate:web": "tsx scripts/generate-web.ts",
    "generate:metadata": "tsx scripts/generate-metadata.ts",
    "generate": "npm run organize && npm run generate:native && npm run generate:web && npm run generate:metadata",
    "build": "tsc",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run generate && npm run build"
  },
  "keywords": [
    "danish",
    "road-signs",
    "traffic-signs",
    "denmark",
    "react",
    "react-native",
    "expo",
    "nextjs",
    "svg"
  ],
  "author": "Yonas Valentin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YonasValentin/danish-road-signs"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-native-svg": "^15.0.0"
  },
  "peerDependenciesMeta": {
    "react-native-svg": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "typescript": "^5.0.0",
    "tsx": "^4.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
